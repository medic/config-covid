      <instance>
        <covid_rdt delimiter="#" id="covid_rdt" prefix="J1!covid_rdt!" version="2020-03-22  18-38">
          <inputs tag="hidden">
            <source>user</source>
            <source_id/>
            <user/>
            <contact>
              <name/>
              <date_of_birth/>
              <sex/>
              <external_id/>
            </contact>
          </inputs>
          <patient_id tag="hidden"/>
          <has_symptoms tag="hidden"/>
          <test_result tag="hidden"/>
          <symptoms_group tag="hidden">
            <symptoms/>
            <generated_note_name_19/>
          </symptoms_group>
          <precautions_group tag="hidden">
            <precautions/>
          </precautions_group>
          <preparations_group tag="hidden">
            <preparations/>
          </preparations_group>
          <finger_group tag="hidden">
            <finger/>
            <generated_note_name_29/>
          </finger_group>
          <disinfect_group>
            <generated_note_name_32/>
            <generated_note_name_33/>
            <generated_note_name_34/>
          </disinfect_group>
          <wait_group tag="hidden">
            <evaluation_timer>900</evaluation_timer>
            <waiting/>
          </wait_group>
          <interpret_group tag="hidden">
            <generated_note_name_41/>
            <interpretation/>
          </interpret_group>
          <cleanup_group tag="hidden">
            <cleanup/>
          </cleanup_group>
          <summary-group>
            <no_symptoms/>
            <positive_result/>
            <negative_result/>
            <invalid_result/>
            <style/>
          </summary-group>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </covid_rdt>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/covid_rdt/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/covid_rdt/inputs/source" type="string"/>
      <bind nodeset="/covid_rdt/inputs/source_id" type="string"/>
      <bind nodeset="/covid_rdt/inputs/contact/name" type="string"/>
      <bind nodeset="/covid_rdt/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/covid_rdt/inputs/contact/sex" type="string"/>
      <bind nodeset="/covid_rdt/inputs/contact/external_id" type="int"/>
      <bind calculate="../inputs/contact/_id" jr:requiredMsg="jr:itext('/covid_rdt/patient_id:jr:requiredMsg')" nodeset="/covid_rdt/patient_id" required="true()" type="string"/>
      <bind calculate="if( /covid_rdt/symptoms_group/symptoms ,'true','false')" nodeset="/covid_rdt/has_symptoms" required="true()" type="string"/>
      <bind calculate="if(selected( /covid_rdt/interpret_group/interpretation ,'c'),if(selected( /covid_rdt/interpret_group/interpretation ,'m') or selected( /covid_rdt/interpret_group/interpretation ,'g'),'positive','negative'),'invalid')" nodeset="/covid_rdt/test_result" type="string"/>
      <bind nodeset="/covid_rdt/symptoms_group/symptoms" type="select"/>
      <bind nodeset="/covid_rdt/symptoms_group/generated_note_name_19" readonly="true()" type="string"/>
      <bind nodeset="/covid_rdt/precautions_group" relevant=" /covid_rdt/has_symptoms ='true'"/>
      <bind jr:constraintMsg="jr:itext('/covid_rdt/precautions_group/precautions:jr:constraintMsg')" nodeset="/covid_rdt/precautions_group/precautions" required="true()" type="select"/>
      <bind nodeset="/covid_rdt/preparations_group" relevant=" /covid_rdt/has_symptoms ='true'"/>
      <bind jr:constraintMsg="jr:itext('/covid_rdt/preparations_group/preparations:jr:constraintMsg')" nodeset="/covid_rdt/preparations_group/preparations" required="true()" type="select"/>
      <bind nodeset="/covid_rdt/finger_group" relevant=" /covid_rdt/has_symptoms ='true'"/>
      <bind jr:constraintMsg="jr:itext('/covid_rdt/finger_group/finger:jr:constraintMsg')" nodeset="/covid_rdt/finger_group/finger" required="true()" type="select"/>
      <bind nodeset="/covid_rdt/finger_group/generated_note_name_29" readonly="true()" type="string"/>
      <bind nodeset="/covid_rdt/disinfect_group" relevant=" /covid_rdt/has_symptoms ='true'"/>
      <bind nodeset="/covid_rdt/disinfect_group/generated_note_name_32" readonly="true()" type="string"/>
      <bind nodeset="/covid_rdt/disinfect_group/generated_note_name_33" readonly="true()" type="string"/>
      <bind nodeset="/covid_rdt/disinfect_group/generated_note_name_34" readonly="true()" type="string"/>
      <bind nodeset="/covid_rdt/wait_group" relevant=" /covid_rdt/has_symptoms ='true'"/>
      <bind nodeset="/covid_rdt/wait_group/evaluation_timer" readonly="true()" required="true()" type="string"/>
      <bind jr:constraintMsg="jr:itext('/covid_rdt/wait_group/waiting:jr:constraintMsg')" nodeset="/covid_rdt/wait_group/waiting" type="select"/>
      <bind nodeset="/covid_rdt/interpret_group" relevant=" /covid_rdt/has_symptoms ='true'"/>
      <bind nodeset="/covid_rdt/interpret_group/generated_note_name_41" readonly="true()" type="string"/>
      <bind nodeset="/covid_rdt/interpret_group/interpretation" type="select"/>
